<!doctype html>
<html>
  <head>
    <title>Let's Use EJS</title>
  </head>
  <body>
    
    <% for(var i=0; i<groceryitems.length; i++){ %>
     <h1> <%= groceryitems[i].store %> Inventory </h1>
     <ul>
          <% groceryitems[i].list.forEach(function(item, index){ %>
               <li><%= item %></li>
          <% }); %>
     </ul>
     <% }; %>



    </body>
</html>